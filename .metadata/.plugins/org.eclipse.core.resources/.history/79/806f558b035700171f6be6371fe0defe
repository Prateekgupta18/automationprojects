        driver.findElement(By.name("pass")).sendKeys("t3stf1rst");
        driver.findElement(By.xpath(".//*[@id='content']/form/div/div/div[1]/button")).click();
        
        driver.manage().timeouts().implicitlyWait(10, TimeUnit.SECONDS);
        
        driver.findElement(By.xpath(".//*[@id='mainnav']/div[6]/a")).click();
        driver.switchTo().frame("accreditationiframe");
        driver.manage().timeouts().implicitlyWait(10, TimeUnit.SECONDS);
        driver.findElement(By.xpath("//li[@id='tab4']")).click();
        
